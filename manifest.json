{
  "name": "Syntaxical!",
  "description": "Highlight dem codes",
  "version": "1.0",
  "background_page": "background.html",
  "options_page": "options.html",
  "permissions": [
    "tabs", "https://*/*", "http://*/*"
  ],  
  "content_scripts": [
	{ 
		"matches": ["http://*/*.cs", "https://*/*.cs", "file:///*/*.cs"], 
		"js": ["scripts/shCore.js", "scripts/shBrushCSharp.js"],
		"run_at": "document_end"
	},
    { 
		"matches": ["http://*/*", "https://*/*", "file:///*/*"], 
		"js": ["content_script.js"],
		"run_at": "document_end"
	}
  ],
  "browser_action": {
      "default_title": "Thats Syntaxical!",
      "default_icon": "icon.png"
  }
}